{
    "name": "bajomodavid/jwt-auth",
    "description": "Simple Jwt Authentication for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bajomo David Mayowa",
            "email": "bajomomayowa@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "lcobucci/jwt": "^3.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BajomoDavid\\JWTAuth\\JWTAuthServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.2.1-dev"
        },
        "aliases": {
            "BDUser": "BajomoDavid\\JWTAuth\\Facade\\BDUser"
        }
    },
    "autoload": {
        "psr-4": {
            "BajomoDavid\\JWTAuth\\": "src/"
        }
    }
}
